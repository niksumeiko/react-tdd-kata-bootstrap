{
  "name": "user-auth-react-tdd-kata",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm api\" \"npx vite\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "api": "node api.cjs",
    "test:unit": "vitest run",
    "test:unit:coverage": "vitest run --coverage",
    "test:integration": "cypress open --component --browser electron",
    "test:e2e": "cypress open --e2e --browser electron",
    "test:e2e:focus": "cypress open --e2e --browser electron --env TAGS=@focus"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.0.18",
    "@vitest/coverage-v8": "^0.34.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "react-router-dom": "^6.20.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@cypress/browserify-preprocessor": "^3.0.2",
    "@hookform/resolvers": "^3.3.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tanstack/react-query": "^5.9.0",
    "@types/cypress-cucumber-preprocessor": "^4.0.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cypress": "^13.6.0",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "dotenv": "^16.3.1",
    "eslint": "^8.54.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-module-utils": "^2.8.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-internal": "file:packages/eslint-plugin-internal",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "tsify": "^5.0.4",
    "typescript": "^5.3.2",
    "vite": "^5.0.4",
    "vitest": "^0.34.6"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/tests/"
  }
}
